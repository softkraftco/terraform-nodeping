terraform {
  required_providers {
    nodeping = {
      version = "0.0.1"
      source  = "softkraft.co/terraform/nodeping"
    }
  }
}

resource "nodeping_contact" "first_contact"{
	custrole = "owner"
	name = "First altered"
	addresses {
		address = "first@o1.com"
		type = "email"
	}
}


output "first_contact_id" {
	value = nodeping_contact.first_contact.id
}
