terraform {
  required_providers {
    nodeping = {
      version = "0.0.1"
      source  = "softkraft.co/terraform/nodeping"
    }
  }
}

resource "nodeping_schedule" "fitst_schedule"{
	name = "FirstSchedule"
	data {
		day = "sunday"
		time1 = "6:00"
		time2 = "7:00"
		exclude = false
	}
}


output "first_schedule_name" {
	value = nodeping_schedule.fitst_schedule.name
}
